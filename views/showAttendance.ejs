    <meta name="viewport" content="width=device-width, initial-scale=1.0">

</head>
<body>
<header>
    <a style="color: white;" href="http://localhost:8080/getDataForWorker"> <button   style="border-collapse: collapse; color: white;" class="btn btn-success">Back</button></a>

</header>

<center>
<div class="container">    <table class="table" id="table">

        <thead class="thead-dark">
          <tr>
            <th >Name</th>
            <th>Time</th>
            <th>Date</th>
            <th>Attendance</th>
            <th>Delete</th>

        </tr>

        </thead>
        <tbody>

        <%dataList.forEach(data =>{%>

          <tr>
            <td><%= data.Name %></td>
            <td><%= data.Time %></td>
            <td><%= data.Date %></td>
            <td><%= data.Attendance %>/30</td>
            <td><button type="button" class="btn btn-danger" onclick="SomeDeleteRowFunction()">Delete</button>
            </td>
            <%})%>
        </tbody>
          </tr>
      </table>
    </div>

     
    
    

</center>

<script>



function SomeDeleteRowFunction() {
      // event.target will be the input element.
      var td = event.target.parentNode; 
      var tr = td.parentNode; // the row to be removed
      console.log(tr.innerHTML)
      tr.parentNode.removeChild(tr);
}

















</script>

</body>
</html>

